{
    "manifest_version": 2,
    "name": "Private Chrome",
    "description": "This extension provides a secure and private way to browse",
    "version": "1.0",
    "browser_action": {
	"default_icon": "icon.png",
	"default_title" : "Encrypt the page",
	"default_popup": "popup.html"
    },
    "content_scripts": [
	{
	    "matches" : [
	    "http://*/*",
	    "https://*/*",
	    "file://*/*"
	    ],
	    "js" : [
		/*"aes.js",
		"tripledes.js",
		"sha512.js",*/
		"main.js"
	    ],
	    "run_at" : "document_idle",
	    "all_frames" : false
	}
    ],  
    "permissions": [
	"tabs"/*,
	"runtime"*/
    ]
}
